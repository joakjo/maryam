CREATE TABLE IF NOT EXISTS point(_id INTEGER PRIMARY KEY AUTOINCREMENT, pointId INTEGER, title TEXT, place INTEGER, radius INTEGER, latitude REAL, longitude REAL, routeId INTEGER)
CREATE TABLE IF NOT EXISTS userdata(_id INTEGER PRIMARY KEY AUTOINCREMENT, userId INTEGER, starPosX REAL, starPosY REAL, message TEXT, sender TEXT, routeId INTEGER, date TEXT, seenAnimation INTEGER, userlang TEXT)
CREATE TABLE IF NOT EXISTS visited(_id INTEGER PRIMARY KEY AUTOINCREMENT, visited INTEGER)
CREATE TABLE IF NOT EXISTS states(_id INTEGER PRIMARY KEY AUTOINCREMENT, stateId INTEGER, stateValue INTEGER)
CREATE TABLE IF NOT EXISTS starPosition(_id INTEGER PRIMARY KEY AUTOINCREMENT, xPos REAL, yPos REAL)
